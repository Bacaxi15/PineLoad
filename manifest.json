{
    "manifest_version": 3,
    "version": "0.1.0",
    "name": "PineLoad",
    "description": "v0.1.0-alpha – Detecta vídeos na página atual",
    "author": "Bacaxi15",
    "action": {
        "default_title": "PineLoad",
        "default_popup": "index.html"
    },

    "permissions": [
        "activeTab",  
        "scripting"
    ],

    "host_permissions":  [
        "<all_urls>"
    ]
}